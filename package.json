    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "npm-check-updates": "^16.3.15",
    "tsconfig-paths": "^4.1.0",
    "prettier": "^2.7.1",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier-plugin-organize-imports": "^3.1.1"
